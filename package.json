{
  "name": "@openmrs/esm-styleguide",
  "version": "1.3.0",
  "description": "The styleguide for OpenMRS SPA",
  "main": "dist/openmrs-esm-styleguide.js",
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "start-https": "webpack-dev-server --mode=development --https",
    "storybook": "start-storybook --port 7000",
    "build-storybook": "build-storybook",
    "lint": "eslint src",
    "build": "webpack",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "directories": {
    "lib": "dist"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'npm:test' 'npm:lint'"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-styleguide.git"
  },
  "keywords": [
    "openmrs",
    "styleguide"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-styleguide/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-styleguide#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@pickra/copy-code-block": "^1.1.7",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/html": "^5.2.1",
    "@types/jest": "^25.2.2",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.6",
    "browserslist-config-openmrs": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "ejs": "^2.6.2",
    "eslint": "^6.1.0",
    "eslint-config-important-stuff": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "^4.2.0",
    "highlight.js": "^9.15.10",
    "husky": "^3.0.0",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "kremling": "^2.0.1",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.0.0",
    "systemjs-webpack-interop": "^1.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "highlight.js": "^9.15.10",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rxjs": "^6.5.3"
  }
}
