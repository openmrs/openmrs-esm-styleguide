{
  "name": "@openmrs/esm-styleguide",
  "version": "1.1.0",
  "description": "The styleguide for OpenMRS SPA",
  "main": "dist/openmrs-esm-styleguide.js",
  "scripts": {
    "start": "webpack-dev-server",
    "storybook": "start-storybook --port 7000",
    "build-storybook": "build-storybook",
    "lint": "eslint src",
    "build": "webpack",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-styleguide.git"
  },
  "keywords": [
    "openmrs",
    "styleguide"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-styleguide/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-styleguide#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@pickra/copy-code-block": "^1.1.7",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/html": "^5.1.9",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-loader": "^8.0.6",
    "browserslist-config-openmrs": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "ejs": "^2.6.2",
    "eslint": "^6.1.0",
    "eslint-config-important-stuff": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "^4.2.0",
    "highlight.js": "^9.15.9",
    "husky": "^3.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {}
}
